server 
{
    listen        8080 9090 ;
    server_name   zzz ;
    error_pages   404.html 500.html ;
    client_body_size  100 ;
    root    stuff ;
 
    location ./dir
    {
        path  ./my-webserv ;
        default_file  form.html ;
        methods  GET  POST DELETE ;
        cgi_bin  /usr/bin/python3 ;
        cgi_extension  .ayac ;
    }

}

server 
{
    listen        8000 ;
    server_name   localhost ;
    error_pages   404.html 500.html ;
    client_body_size  100000 ;
    root    server_3 ;

    location ./tEst/
    {
        path  ./my-webserv ;
        default_file  form.html ;
        methods  GET  POST DELETE ;
        cgi_bin  /usr/bin/python3 ;
        cgi_extension  .ayac ;
    }
}

server
{
    listen        6060 ;
    server_name   127.0.0.1 ;
    error_pages   404.html  500.html ;
    client_body_size  50000 ;
    root    ./test ;
    location /etc1
    {
        default_file  index.html ;
        methods  GET   DELETE ;
        cgi_bin  /usr/bin/python3 ;
        cgi_extension  .py ;
    }
    location ./test/
    {
        default_file  iNdex.html ;
        methods  GET  POST DELETE ;
        cgi_bin  /usr/bin/python4 ;
        cgi_extension  .BRUH ;
    }
    location ./MAMA/
    {
        default_file  inDex.html ;
        methods  GET  POST DELETE ;
        cgi_bin  /usr/bin/python4 ;
        cgi_extension  .ZOOT ;
    }
}
