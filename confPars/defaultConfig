server
{
    listen        8080 ;
    server_name   example.com ;
    host            127.0.0.1 ;
    error_pages   404.html 405.html 403.html ;
    client_body_size  5000000 ;
    default_file    index.html ;
    root    stuff ;
    autoindex   on ;
    location /dir/
    {
        default_file  default.html ;
        methods GET DELETE ;
        cgi_extension   .py ;
        cgi_bin  /usr/bin/python3 ;
    }
    location /upload/
    {
        default_file  index1.html ;
        methods GET POST DELETE ;
        cgi_extension   .py ;
        cgi_bin  /usr/bin/python3 ;
    }
    location /test/
    {
        default_file  iNdex2.html ;
        methods  GET  POST DELETE ;
        cgi_bin  /usr/bin/python4 ;
        cgi_extension  .BRUH ;
        return 301 /dir/ ;
    }
    location /kkk/
    {
        alias dir/lala.html ;
        default_file  inDex3.html ;
        methods  GET  POST DELETE ;
        cgi_bin  /usr/bin/python4 ;
        cgi_extension  .ZOOT ;
    }
}
